#include<iostream>
using namespace std;
int main()
{
    int l,w,h,n,i;
    cin>>l;
    cin>>n;
    for(i=0;i<n;i++)
    {
        cin>>w>>h;
        if(w<l || h<l )
        {
        cout<<"UPLOAD ANOTHER"<<endl;
        }
        if(w>=l && h>=l)
        {
         if(w==h )
        {
            cout<<"ACCEPTED"<<endl;
        }
        else
        cout<<"CROP IT"<<endl;
        }
    }
    return 0;
}
