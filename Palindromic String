#include <iostream>
#include <string.h>

using namespace std;

int main()
{
    int n,i;
	string str;
	getline(cin , str);
	n=str.size();
	int k=0;
	for(i=0;i<n;i++)
	{
	    if (str[i]!=str[n-1-i])
	        k=1;
	        break;
	}
	if (k==0)
        cout<<"YES";
    else
        cout<<"NO";
	return 0;

}
