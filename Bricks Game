#include <iostream>
using namespace std;
int main() 
{
    int n,i;
    cin>>n;
    int a=n;
    for(i=1;i<n;i++)
    {
        a=a-i;
        if(a<=0)
        {
            cout<<"Patlu";
            break;
        }
        a=a-2*i;
        if(a<=0)
        {
            cout<<"Motu";
            break;
        }
    }
    
    return 0;   
}
