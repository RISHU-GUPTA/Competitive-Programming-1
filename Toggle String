#include<iostream>
using namespace std;
int main()
{
    string s1;
    int n,i;
    getline(cin,s1);
    n=s1.size();
    for( i=0;i<n;i++)
    {
        if(s1[i]>=97 && s1[i]<=122)
        {
            s1[i]=s1[i]-32;
        }
        else
        {
            s1[i]=s1[i]+32;
        }
         cout<<s1[i];
        
    }
    return 0;
}
