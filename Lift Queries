#include <iostream>
#include<math.h>
using namespace std;
int main() 
{
	int n,x,i;
	cin >> n;
	int a=0,b=7;
	for(i=0;i<n;i++)
	{
	    cin>>x;
	    if(abs(x-a)>abs(x-b))
	    {
	        cout<<"B"<<endl;
	        b=x;
	    }
	    else if(abs(x-a)<abs(x-b))
	    {
	        cout<<"A"<<endl;
	        a=x;
	    }
	    else
	    {
	        cout<<"A"<<endl;
	        a=x;
	    }

	}
	return 0;
	
}

